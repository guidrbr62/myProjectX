<script>
    $('span.addDownloadedBook').on("click", function(event) {
        CurrentUser.showAuthPopup('login')
    })
    $('.dropdown-menu.mylibrary-dropdown-menu').on("click", function(event) {
        event.stopPropagation()
    })

    function checkIsInMyLibrary(options) {
        Promise.all([
            options?.isFavorite !== undefined ? options?.isFavorite : new UserBookmarks(CurrentUser.id).checkBookIsSaved({
                bookId: CurrentBook.id
            }),
            options?.isBooklisted !== undefined ? options?.isBooklisted : new UserBookmarks(CurrentUser.id).checkBookIsInBooklist({
                bookId: CurrentBook.id
            })
        ]).then(values => {
            if (values[0] || values[1]) {
                document.querySelector('.btn-mylibrary .mylibrary-icon')?.classList.replace('zlibicon-mylibrary', 'zlibicon-mylibrary-fill')
            //    document.querySelector('.dropdown-mylibrary-icon.icon-heart')?.classList.replace('zlibicon-heart', 'zlibicon-heart-fill')
            } else {
                document.querySelector('.btn-mylibrary .mylibrary-icon')?.classList.replace('zlibicon-mylibrary-fill', 'zlibicon-mylibrary')
            }
        })
    }

    if (CurrentUser.id) {
        checkIsInMyLibrary()

        new UserBookmarks(CurrentUser.id).checkBookIsSaved({
            bookId: CurrentBook.id
        }).then((result) => {
            if (result == true) {
                $('.dropdown-mylibrary-favorite .dropdown-mylibrary-icon').removeClass('zlibicon-heart').addClass('zlibicon-heart-fill')
            }
        })

        document.querySelector('.dropdown-mylibrary-favorite').addEventListener("click", function() {
            const id = 23554619;
            new UserBookmarks(CurrentUser.id).checkBookIsSaved({
                bookId: id
            }).then((result) => {
                if (result) {
                    new UserBookmarks(CurrentUser.id).deleteBookFromSavedBooks({
                        bookId: id,
                        success: function() {
                            $('.dropdown-mylibrary-favorite .dropdown-mylibrary-icon').removeClass('zlibicon-heart-fill').addClass('zlibicon-heart')
                            checkIsInMyLibrary({
                                isFavorite: false
                            })
                        }
                    })
                } else {
                    new UserBookmarks(CurrentUser.id).addBookToSavedBooks({
                        bookId: id,
                        success: function() {
                            $('.dropdown-mylibrary-favorite .dropdown-mylibrary-icon').removeClass('zlibicon-heart').addClass('zlibicon-heart-fill')
                            checkIsInMyLibrary({
                                isFavorite: true
                            })
                        }
                    })

                }
            })
        })
    } else {
        document.querySelector('.btn-mylibrary').addEventListener("click", function(e) {
            e.stopPropagation()
            const text = i18next.t('After authorization, you will be able to save books using the «Add to Favorites» option')
            new User().showAuthPopup('login', text)
        })
    }
    document.addEventListener('favoritesUpdated', () => {
        new UserBookmarks(CurrentUser.id).checkBookIsSaved({
            bookId: CurrentBook.id
        }).then((result) => {
            if (result == true) {
                $('.dropdown-mylibrary-favorite .dropdown-mylibrary-icon').removeClass('zlibicon-heart').addClass('zlibicon-heart-fill')
            }
        })
        checkIsInMyLibrary()
    })
</script>
