<script type="text/javascript">
    const $tagsInput = $('#inputTags')
    const tags = $tagsInput.length ? new TagsInput($tagsInput, CurrentUser, CurrentBook.id, availableTags) : null

    // deactivate links if JS available
    $('.no-js__link').removeAttr("href");

    let showAddCategoryTooltip = false;
        showAddCategoryTooltip = true;
    let showPremiumReadOnlineTooltip = true
    // flag for second tooltip in modal window (click on `Add a category` => second tooltip is no needed)
    var showTooltipTwice = true

    let reportWidgetStatus = {
        pending: false,
        open: false,
    };
    document.addEventListener('zmb__state-changed', ({detail}) => {
        const {state, role} = detail;
        if (role === ZModerationBlock.availableRoles.bookIssueFeedback) {
            reportWidgetStatus.pending = false;
            switch (state) {
                case 'open':
                    reportWidgetStatus.open = true;
                    break;
                case 'closed':
                    reportWidgetStatus.open = false;
                    break;
            }
        }
    });
    function openSuggestWidget() {
        showTooltipTwice = true
        CurrentBook.suggestModal()
    }
    function openReportWidget() {
        if (!reportWidgetStatus.open && !reportWidgetStatus.pending) {
            reportWidgetStatus.pending = true;
            dispatchCustomEvent('book-report-requested');
        }
    }

    const addCatLink = $('#add_category_link'),
        containerCatLinkTootip = $('.property_categories .property_value'),
        dlButton = $("a.dlButton"),
        containerDlButtonTooltip = $("div.book-details-button")

    addCatLink.on('click', () => {
        showTooltipTwice = false
        addCatLink.tooltip("hide")
        CurrentBook.suggestModal()
    })
    // showAddCategoryTooltip unsets by clicking on link on the tooltip
    if (showAddCategoryTooltip) {
        // manual trigger: dont hide on mouse over tooltip
        addCatLink.tooltip({
            html: true,
            title: i18next.t("Do you want to help us? We are replenishing the database with categories, so we need your help.") +
                '<br/><a href="javascript://" id="hide_tooltip_add_category">' + i18next.t("Don't show again") + '</a>',
            trigger: 'manual',
        });
        addCatLink.find(".tooltip").css("padding", "25px")
        addCatLink
            // show tooltip on mouseover; hide if mouse leave tooltip
            .on('mouseenter', () => {
                if (!showAddCategoryTooltip) return
                // dont show on mobiles
                if (document.body.clientWidth < 768) return
                // show the tooltip if it is not shown
                setTimeout(() => {
                    if (!containerCatLinkTootip.find('.tooltip').length) {
                        addCatLink.tooltip('show')
                    }
                }, 300)
                // don't show again link click event
                addCatLink.on('shown.bs.tooltip', () => {
                    // hide on click  `don't show again` and save attribute
                    $('#hide_tooltip_add_category').on('click', () => {
                        showAddCategoryTooltip = false
                        addCatLink.tooltip('hide')
                        new ZLibraryResponse('/papi/user/attribute/hideAddCategoryTooltip').fetch()
                    })
                    // close the tooltip if mouse leave it and mouse is not over link `add a category`
                    containerCatLinkTootip.find('.tooltip').on('mouseleave', () => setTimeout(() => {
                        if (!containerCatLinkTootip.find('a:hover').length) {
                            addCatLink.tooltip('hide')
                        }
                    }, 300))
                })
            })
            // hide tooltip if mouse leave link and it's not over tooltip
            .on('mouseleave', () => setTimeout(() => {
                if (!containerCatLinkTootip.find('.tooltip:hover').length) {
                    addCatLink.tooltip('hide')
                }
            }, 300))

        if (!$('.z-moderation-block').data('active')) {
            // show tooltip on download and convert
            $("a.dlButton" + ", " +
                ".btn-group a.converterLink" + ", " +
                "#sendToEmailButtonBox a.kindle__sendFileBtn, #sendToEmailButtonBox a.kindle__convertAndSend" + ", " +
                "#sendToButtonBox .send-to-widget .menu-row")
                .on('click', () => {
                    if (!showAddCategoryTooltip) return
                    // some delay, because body.onclick hides tooltip
                    setTimeout(() => addCatLink.tooltip('show'), 200)
                    // hide after 15 sec after click
                    setTimeout(() => addCatLink.tooltip('hide'), 15000)
                })
            // close immediatly on any click
            $("body").on('click', () => addCatLink.tooltip('hide'))
        }
    }

        // function setDownloadListener(){
    //     $(".btn-group li a.addDownloadedBook").on('click', () => {

    //     })
    // }
    $(function () {
        $('#btnCheckOtherFormats').click(function() {
            if (CurrentBook.otherFormatsLoaded) {
                return
            }
            const $that = $(this)
            const spinner = new ZLibrarySpinner('btnCheckOtherFormats').length(3).radius(2).width(2).start()
            var isReadOnline = $that.data('check-formats') == 2

            CurrentBook.checkOtherFormats(isReadOnline).then(() => {
                spinner.stop()
                $that.attr('data-toggle', 'dropdown').trigger('click')
                // set an event listener after other formats loading
                                if (!showAddCategoryTooltip) return
                addCatLink.tooltip('show')

            }).catch(() => {
                spinner.stop()
                $that.attr('data-toggle', 'dropdown').trigger('click')
            })
            CurrentBook.otherFormatsLoaded = true
        })

        // read more
        var height = 300;
        if ($('.termsCloud').height() > 0)
        {
            height = height - $('.termsCloud').height();
        }

        if (height < 225) {
            height = 225; // min height
        }

        $bookDescriptionBox = $('#bookDescriptionBox')
        // prevent bad line-brake
        height = Math.floor(height / parseFloat($bookDescriptionBox.css('line-height'))) * parseFloat($bookDescriptionBox.css('line-height')) + 10; //10 - padding-bottom of box
        
        if ($bookDescriptionBox.prop('scrollHeight') > height)
        {
            $bookDescriptionBox.css('overflow', 'hidden');
            $bookDescriptionBox.css('height', height);
            $('<div class="book-read-more load-more moreBtn"><div class="load-more-text">Mais detalhes<i class="zlibicon-arrow-down-n"></i></div></div>').insertAfter("#bookDescriptionBox");
        }else{
            $bookDescriptionBox.addClass('expanded')
        }

        $('.moreBtn, #bookDescriptionBox').click(function () {
            $bookDescriptionBox.css('height', $bookDescriptionBox.prop('scrollHeight') + 'px');
            $bookDescriptionBox.addClass('expanded');
            $bookDescriptionBox.on('transitionend', ()=>{
                $bookDescriptionBox.css('height', 'auto');
                $bookDescriptionBox.css('overflow', 'auto');
            })
            $moreBtn = $('.moreBtn')
            $moreBtn.css('opacity', 0);
            $moreBtn.css('marginBottom', '-32px');
            $moreBtn.on('transitionend', ()=>{
                $moreBtn.remove();
            })
            
        });

        // Bookmarks
        if ($('.bookmarks').length) {
            userBookmarksInstance = new UserBookmarks(CurrentUser.id)
            userBookmarksInstance.init({
                page: 'book',
                savedBookTags: [],
            })
        }

    })

    $(window).on("load", function () {
        const dibaledDownloadId = "#dbx_" + 23554619;
        $('[data-toggle="tooltip"]').tooltip({'html': true});
        $('[data-autoopen="true"]').tooltip('show');

        $('body').click(()=>{
            $(dibaledDownloadId).tooltip('hide');
        })
        $(".book-details-button").on("mouseenter", ()=>{
            $(dibaledDownloadId).tooltip('show');
        })
        $(".book-details-button").on("mouseleave", ()=>{
            $(dibaledDownloadId).tooltip('hide');
        })

        $('.btn-savebook-disabled').tooltip({
            'html': true,
            'trigger': 'manual',
        });

        $('.btn-savebook-disabled').mouseover(function () {
            $(this).tooltip('show')
        });

        $('.btn-savebook-disabled').click(function () {
            $(this).tooltip('hide')
        });
    });

    function iOSversion()
    {
        if (/iP(hone|od|ad)/.test(navigator.userAgent)) {
            const v = (navigator.appVersion).match(/OS (\d+)_(\d+)_?(\d+)?/)
            return [parseInt(v[1], 10), parseInt(v[2], 10), parseInt(v[3] || 0, 10)]
        }

        return [];
    }

    const gratitudesIcons = null;

    // $('.icon-gratitude').on('click', function (event) {
    //     // $(this).tooltip('disable');
    //     $(this).tooltip('hide');

    //     $('.booklists-container, .save-for-later-container').hide();

    //     event.stopPropagation();
    // })

    // $(window).click(function() {
    //     $('.icon-gratitude').tooltip('enable')
    // });

    $('.addDownloadedBook, .converterLink, .menu-row-link a').on('click', function() {
        const isGratitudesTooltipShown = localStorage.getItem('isGratitudesTooltipShown')

        $('.js-gratitude-icon').removeClass('_disabled hidden')

        if (!isGratitudesTooltipShown) {
            $('.icon-gratitude').tooltip({html: true, trigger: 'manual'})
            $('.icon-gratitude').tooltip('show')
            localStorage.setItem('isGratitudesTooltipShown', true)

            const timeout = setTimeout(() => {
                $('.icon-gratitude').tooltip('hide')
                clearTimeout(timeout)
            }, 6000);
        }
    })

    $('.reader-link').on('click', function() {
        $('.js-gratitude-icon').removeClass('_disabled hidden')
    })

    if (!isBookDownloaded(+CurrentBook.id)) {
        $('.js-gratitude-icon').attr('data-original-title', 'Para dizer &quot;Obrigado&quot; aos nossos colaboradores, primeiro faça o download do livro ');
        $('.js-gratitude-icon').addClass('_disabled hidden')
    }


    $('.copytext-container').click(function(event) {
        const $input = $('<input type="text" style="width: 1px; height: 1px; border: none; opacity: 0;">')
        $(this).after($input)
        $input.val($(this).html()).focus().select()

        try {
            document.execCommand('copy')
            ZLibraryNotify({delay: 3000}).info('Endereço copiado para')
        } catch (err) {
            ZLibraryNotify({delay: 3000}).error('Ups, a copiar ocorreu um erro')
        }
        $input.remove()
    })
    $(document).on('click', '.tooltip a', function(event) {
        event.preventDefault();
        var linkUrl = $(this).attr('href');
        window.open(linkUrl, '_blank');
    });
</script>

