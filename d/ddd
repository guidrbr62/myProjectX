            <div class="load-more" onclick="loadMore(this, 'recommend')">
            <div class="load-more-text"><span>Carregar mais</span><i class="zlibicon-arrow-down-n"></i></div>
            <img class="loader" src="/img/loader.svg" />
        </div>
    
    <script>
    
        if (document.getElementById('booksMosaicBoxContainer').childNodes.length > 1) {
            document.getElementById('booksMosaicBoxContainer').parentNode.style.display = 'block'
        }
        let mosaicPage = parseInt('2');
        let onPage = parseInt('21');

        const mosaicNode = document.querySelector('.masonry-endless')
        mosaicNode.addEventListener('transitionend', function(){
            mosaicNode.style.height = ''
        })
        function loadMore(button, source) {
            button.classList.add('loading')
            mosaicNode.style.height = mosaicNode.scrollHeight + 'px'
            mosaicPage++
            let URL = `/papi/book/${source}/mosaic/${onPage}/${mosaicPage}`
            let data
            switch (source){
                case "recommend":
                    URL = `/papi/book/recommended/mosaic/${onPage}/${mosaicPage}`
                    data = { bookIds: [11145047,11642757,11973632,16343074,17135299,17135302,18433146,18634351,18634352,21786532,22458164,27496296,28432494,28432560,23554612,23554618,4929406,5324228,5324229,5324231,5848543,28432482,13796590,22458112,3606510,3583928,21413918,12391828,6064928,13181330,2865803,28432530,3716542,11188564,29536663,16564262,27496136,11025668,28432474,16343076,27496146,27496144] }
                    break
            }
            new ZLibraryResponse(URL, data)
                .success(data => {
                    const node = document.querySelector('.masonry-endless')
                    if(data.books){
                        for(book of data.books){                        
                            node.insertAdjacentHTML("beforeend", `<div class="item"><a href="${book.url}?dsource=${source}">
                                <z-cover volume id="${book.id}" isbn="${book.isbn}" author="${book.author}" title="${book.title}">
                                    <img data-src="${book.cover}" alt="${book.author} — ${book.title}">
                                </z-cover>
                            </a></div>`)
                        }
                        buildMosaic()                  
                        button.classList.remove('loading')
                        mosaicNode.style.height = mosaicNode.scrollHeight + 'px'
                    }
                    if(data.nextPage === false || !data.books){
                        document.querySelector('.load-more').style.display = 'none'
                        document.querySelector('.masonry-endless').style.marginBottom = '0px'
                    }
                })
                .fetch()    
        }

        function buildMosaic() {
            const wrap = document.querySelector('.masonry-endless')
            const book = document.querySelector('.masonry-endless .item')
            const books = document.querySelectorAll('.masonry-endless .item')
            const booksLength = document.querySelectorAll('.masonry-endless .item').length
            const wrapWidth = wrap.getBoundingClientRect().width
            const bookWidth = book.getBoundingClientRect().width
            const bookInRow = Math.floor(wrapWidth / bookWidth)
            const booksToHide = booksLength % bookInRow
            books.forEach(node => node.style.display = '')
            if(booksToHide < booksLength){
                for(let i = booksLength; i > booksLength - booksToHide; i--){
                    books[i-1].classList.add = 'full';
                }
            }
        }
        window.addEventListener('resize', buildMosaic)
        document.addEventListener('DOMContentLoaded', buildMosaic)
    
    </script>
</div>

